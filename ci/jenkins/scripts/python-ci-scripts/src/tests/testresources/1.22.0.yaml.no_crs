apiVersion: v1
kind: ConfigMap
metadata:
  name: eric-oss-ves-collector-rsyslog
  labels:
    app: "eric-oss-ves-collector"
    app.kubernetes.io/instance: "eric-oss-adc"
    app.kubernetes.io/managed-by: "Helm"
    app.kubernetes.io/name: "eric-oss-ves-collector"
    app.kubernetes.io/version: "0.0.2-2"
    chart: "eric-oss-ves-collector-0.0.2-2"
    helm.sh/chart: "eric-oss-ves-collector-0.0.2-2"
  annotations:
    ericsson.com/product-name: "VES Collector"
    ericsson.com/product-number: "CXD 101 324"
    ericsson.com/product-revision: "0.0.2"
data:
  rsyslog.conf: |
    #### MODULES ####
    module(load="imuxsock" SysSock.Use="off")
    module(load="mmpstrucdata")

    #### GLOBAL DIRECTIVES ####
    global(preserveFQDN="off"
           )

    #### RULES ####
    input(type="imuxsock" Socket="/run/eea/log" Ruleset="sendToRemote"
          UseSpecialParser="off" IgnoreTimestamp="off" CreatePath="on")

    template(name="file_name" type="string" string="/data/eea_log/%procid%_%$!rfc5424-sd!extra_data@193!inst%.log")

    template(name="msg_template" type="list") {
        property(name="timereported" dateFormat="year")
        constant(value="-")
        property(name="timereported" dateFormat="month")
        constant(value="-")
        property(name="timereported" dateFormat="day")
        constant(value=" ")

        property(name="timereported" dateFormat="hour")
        constant(value=":")
        property(name="timereported" dateFormat="minute")
        constant(value=":")
        property(name="timereported" dateFormat="second")
        constant(value=".")
        property(name="timereported" fixedwidth="on" position.to="6" dateFormat="subseconds")
        constant(value=" ")

        constant(value="@")
        property(name="$!rfc5424-sd!extra_data@193!tid")
        constant(value=" ")

        constant(value="(")
        property(name="syslogseverity-text" caseConversion="upper")
        constant(value=") ")

        property(name="msg" droplastlf="on" )
        constant(value="\n")
    }

    ruleset(name="sendToRemote" parser="rsyslog.rfc5424") {
        action(type="mmpstrucdata")
        action(type="omfile" dirCreateMode="0700" FileCreateMode="0644" dynaFile="file_name" template="msg_template")
        action(type="omfile" File="/proc/1/fd/2" template="msg_template")
        action(type="omfwd" target="eric-log-transformer" port="5014" protocol="tcp"
               template="RSYSLOG_SyslogProtocol23Format")
    }
